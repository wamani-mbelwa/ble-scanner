{
  "name": "react-native-ble-scanner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "build:android": "cd android && ./gradlew assembleDebug",
    "e2e:detox:build:android": "detox build -c android.emu.debug",
    "e2e:detox:test:android": "detox test -c android.emu.debug -w 1"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.9.27",
    "react": "18.2.0",
    "react-native": "0.75.3",
    "react-native-ble-plx": "^2.0.3",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^4.8.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/runtime": "^7.25.6",
    "@react-native/eslint-config": "^0.76.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-native": "^0.75.1",
    "babel-jest": "^29.7.0",
    "detox": "^20.17.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.4",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^15.2.10",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.3.3",
    "react-native-testing-library": "^2.1.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ]
  }
}
